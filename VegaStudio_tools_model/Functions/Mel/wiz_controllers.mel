// slidr maker



global proc make()

{

if (`window -exists sm`)

deleteUI -window sm;


window -title "slider maker" -rtf 1 sm;

columnLayout -adjustableColumn true sm;

frameLayout -w 180 -label " Border" -borderStyle "etchedIn" Border;


columnLayout -adjustableColumn true;

separator -h 10 -style "none";

intSliderGrp -field true -label "Height" -value 5 -maxValue 10 -fieldMaxValue 100 -columnWidth3 50 30 50 -adjustableColumn 3 height;

intSliderGrp -field true -label "Width " -value 5 -maxValue 10 -fieldMaxValue 100 -columnWidth3 50 30 50 -adjustableColumn 3 width;


setParent..;

setParent..;

separator -h 5 -style "none";

frameLayout -w 180 -label " Handle" -borderStyle "etchedIn" Handle;

columnLayout -adjustableColumn true;

separator -h 10 -style "none";

floatSliderGrp -field true -label "Size" -value 1 -maxValue 10 -fieldMaxValue 100 -columnWidth3 50 30 50 -adjustableColumn 3 size;

setParent..;

setParent..;

separator -h 5 -style "none";

button -label "Make Slider " -h 30 -c s ;


showWindow sm;

}


global proc s ()

{

string $h = `intSliderGrp -q -value height`;

string $w = `intSliderGrp -q -value width`;

int $fh = $h;

int $fw = $w;

string $bor = `curve -d 1 -p -1 1 0 -p 1 1 0 -p 1 -1 0 -p -1 -1 0 -p -1 1 0 -k 0 -k 8 -k 16 -k 24 -k 32` ;




scale $fw $fh 0;

makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;

setAttr ($bor+".overrideEnabled") 1;

setAttr ($bor+".overrideColor") 6;

string $size = `floatSliderGrp -q -value size`;

circle -c 0 0 0 -nr 0 0 1 -sw 360 -r $size -d 3 -ut 0 -tol 0.01 -s 8 -ch 1 ;



makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;

setAttr ".overrideEnabled" 1;

setAttr ".overrideColor" 17;


delete -ch;


string $selbor [] = `ls -sl`;

for ($objbor in $selbor)

{

if ($fw == 0 )

{

setAttr -lock true -keyable false ($objbor+".tx");

}

if ($fh == 0 )

{

setAttr -lock true -keyable false ($objbor+".ty");

}

setAttr -lock true -keyable false ($objbor+".tz");

setAttr -lock true -keyable false ($objbor+".rx");

setAttr -lock true -keyable false ($objbor+".ry");

setAttr -lock true -keyable false ($objbor+".rz");

setAttr -lock true -keyable false ($objbor+".sx");

setAttr -lock true -keyable false ($objbor+".sy");

setAttr -lock true -keyable false ($objbor+".sz");

setAttr -lock true -keyable false ($objbor+".v");

parent $selbor $bor;

transformLimits -tx (-($fw)) $fw -etx 1 1 $objbor;

transformLimits -ty (-($fh)) $fh -ety 1 1 $objbor;

select -cl ;

}

}


// combine


global proc combine ()

{

string $sl[] = `ls -sl`;

makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;

delete -ch;

string $sh []= `listRelatives -s $sl[0]`;

parent -add -shape $sh $sl[1];

delete $sl[0];

}


