global proc mirrorX()

{

string $copy = `checkBox -q -value cb01`;

get;

mirrorunlock;

if ($copy == 1)

{


string $name01 = `textField -q -text tsName`;

string $name02 = `textField -q -text trName`;


duplicate ;

searchReplaceNames $name01 $name02 "hierarchy";

}

group -n "controllers_mirror_grp";

xform -os -piv 0 0 0;


scale -1 1 1 ;

ungroup controllers_mirror_grp;

string $name01 = `textField -q -text tsName`;

string $name02 = `textField -q -text trName`;

searchReplaceNames $name01 $name02 "hierarchy";

//makeIdentity -apply true -t 0 -r 0 -s 1 -n 0;

lockhide;

}

