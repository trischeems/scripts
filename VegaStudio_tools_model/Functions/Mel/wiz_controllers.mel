// edit lockhide



global proc edit_t()

{

int $tx = `checkBox -q -value tx`;

int $ty = `checkBox -q -value ty`;

int $tz = `checkBox -q -value tz`;


if ($tx + $ty + $tz == 3)

{

checkBox -e -value 1 t;

}

else

{

if ($tx + $ty + $tz == 0)

{

checkBox -e -value 0 t;

}

}

}



global proc edit_r()

{

int $rx = `checkBox -q -value rx`;

int $ry = `checkBox -q -value ry`;

int $rz = `checkBox -q -value rz`;


if ($rx + $ry + $rz == 3)

{

checkBox -e -value 1 r;

}

else

{

if ($rx + $ry + $rz == 0)

{

checkBox -e -value 0 r;

}

}

}



global proc edit_s()

{

int $sx = `checkBox -q -value sx`;

int $sy = `checkBox -q -value sy`;

int $sz = `checkBox -q -value sz`;


if ($sx + $sy + $sz == 3)

{

checkBox -e -value 1 s;

}

else

{

if ($sx + $sy + $sz == 0)

{

checkBox -e -value 0 s;

}

}

}


//controls
//01
    global proc shp01 ()
    {
        string $name = `textField -q -text tfName`;
            curve -d 1 -p -1 1 1 -p -1 1 -1 -p 1 1 -1 -p 1 1 1 -p -1 1 1 -p -1 -1 1 -p -1 -1 -1 -p -1 1 -1 -p -1 1 1 -p -1 -1 1 -p 1 -1 1 -p 1 1 1 -p 1 1 -1 -p 1 -1 -1 -p 1 -1 1 -p 1 -1 -1 -p -1 -1 -1 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 -k 7 -k 8 -k 9 -k 10 -k 11 -k 12 -k 13 -k 14 -k 15 -k 16;
            rename ($name+"_control");
            scale -r 5 5 5;
            makeIdentity -apply true -t 0 -r 0 -s 1 -n 0;
            xform -os -piv 0 0 0;
        {
            string $group = `checkBox -q -value grp01`;
            get;
            if ($group == 1)
            {
                group -n ($name+"_group");
                xform -os -piv 0 0 0;
            }
        }
        group -n ($name+"_null");
        xform -os -piv 0 0 0;
    }

    global proc b01 (){
    string $sel_1[] = `ls -sl`;
        textField -e -text $sel_1 text01;
    string $text = `textField -q -text text01`;
        if ($text == "")
        {
        shp01;
        }
    else
    {
        shp01;
        string $sel_2[] = `ls -sl`;
        string $cons [] = `parentConstraint $sel_1 $sel_2`;
        delete $cons;
    }
    
    set;
    lockhide;

   }

   
