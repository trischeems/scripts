global proc float sec_to_frames(float $seconds)
{
    float $fps = 0.0;
    string $current_time_unit = `currentUnit -q -t`;
    switch($current_time_unit)
    {

        case "game": //15 fps
            $fps = 15.0;
        break;

        case "film": // 24 fps
            $fps =  24.0;
        break;

        case "pal": //25 fps
            $fps =  25.0;
        break;

        case "ntsc": // 30 fps
            $fps =  30.0;
        break;

        case "show": // 48 fps
            $fps =  48.0;
        break;

        case "palf": // 50 fps
            $fps =  50.0;
        break;

        case "ntscf": // 60 fps
            $fps =  60.0;
        break;

        default:
            $fps = (float(substituteAllString($current_time_unit , "fps", ".0")));
        break;


    }

    float $sec_to_frames = ($fps * $seconds);

    print ("\n// " + $fps + " fps: " + $seconds + " seconds = " + $sec_to_frames + " frames");

    return $sec_to_frames;
}


