global proc craOffsetSlider(string $mode)
{
    global int $craOffsetState;

    if(size(`ls -sl`))
    {
        if(size(`craGetAnimated 1 "transform"`))
        {
            // open undo chunk
            if(!$craOffsetState)
            {
                undoInfo -chunkName "craOffsetKeysUndo" -openChunk;
                // set undo chunk state
                $craOffsetState = 1;
            }

            float $value;
            if($mode == " -tc ")
                $value = `floatSlider -q -v craOffsetKeysTimeFS`;
            else
                $value = `floatSlider -q -v craOffsetKeysValueFS`;
            int $sign = `sign($value)`;

            craOffsetKeysWrapper $sign $mode;
        }
        else
            warning "No animated object(s) selected";
    }
    else
        warning "No object(s) selected";
}

global proc craStopOffsetSlider()
{
    global int $craOffsetState;

    floatSlider -e -v 0 craOffsetKeysTimeFS;
    floatSlider -e -v 0 craOffsetKeysValueFS;
    // close undo chunk
    undoInfo -closeChunk;
    // set undo chunk state
    $craOffsetState = 0;
    // evalDeferred "dgdirty -a";
}


global proc craOffsetKeysWrapper( float $direction, string $changeMode )
{
    float $framesOffset = `floatField -q -v craOffsetKeysFramesIF`;

	// int $mods = `getModifiers`;
	// // control+shift
 //    if ($mods == 5)
	// 	$framesOffset *= 0.1;
 //    // control
 //    if ($mods == 4)
 //        $framesOffset *= 0.5;
 //    // shift
	// if ($mods== 1)
	// 	$framesOffset *= 1.5;
 //    // alt
 //    if ($mods == 8)
 //        $framesOffset *= 2.0;

    int $sorting = `optionMenu -q -sl craOffsetKeysSelectionOrderOM`;
    int $mode = 0;
    string $objects[] = `ls -sl -l`;

    if(`iconTextCheckBox -q -v craStaggerOffsetCB`)
        $mode = 1;
    if(`iconTextCheckBox -q -v craRandomOffsetCB`)
        $mode = 2;

    undoInfo -chunkName "craOffsetKeysUndo" -openChunk;
    craOffsetKeysProc( $objects, $framesOffset, $mode, $sorting, $direction, $changeMode);
    undoInfo -closeChunk;
	// if($mods)
	// 	craInViewMessage "Offset value multiplier" $framesOffset;
    // evalDeferred "dgdirty -a";
}

global proc craOffsetKeysProc( string $objects[], float $framesOffset, int $mode, int $sorting, int $direction, string $changeMode)
{
    global string $gPlayBackSlider;

	if(!size($objects))
    {
        if (`confirmDialog -title "Nothing selected" -p "craOffsetKeysWin" -message "Use all animated objects?" -button "Yes" -button "No" -defaultButton "Yes" -cancelButton "No" -dismissString "No"` == "Yes")
        {
            if (`confirmDialog -title "Offset animated" -p "craOffsetKeysWin" -message "Transforms/Joints only?" -button "Yes" -button "No" -defaultButton "Yes" -cancelButton "No" -dismissString "No"` == "Yes")
                $objects = craGetAnimated(1, "transform");
            else
                $objects = craGetAnimated(0, "");

        select -r $objects;
        }
    }

    if(size($objects))
    {
	    string $craOffsetKeysCBAttrs[] =`craOffsetKeysGetCBAttrs`;
	    string $craOffsetKeysAttrs;

	    string $craAnimCurves[] =`keyframe -q -sl -name`;
	    int $craSizeAnimCurves = size(`keyframe -q -sl -name`);

        float $timeRange[] = `timeControl -q -rangeArray $gPlayBackSlider`;
        string $channels[] = `timeControl -q -animCurveNames $gPlayBackSlider`;
        float $keys[] = `keyframe -time ($timeRange[0] + ":" + $timeRange[1])  -query -timeChange $channels`;

        if(size($craOffsetKeysCBAttrs) && !$craSizeAnimCurves)
	    {
	        $craOffsetKeysAttrs = " -at \"";
	        $craOffsetKeysAttrs+= stringArrayToString($craOffsetKeysCBAttrs, "\" -at \"");
	        $craOffsetKeysAttrs+="\" ";
	    }
	    else if($craSizeAnimCurves)
	        $objects = $craAnimCurves;

	    if($sorting == 2)
	        $objects = craOffsetKeysInvertArray($objects);

	    if($sorting == 3)
	        $objects = sort($objects);

	    if($sorting == 4)
	    {
	        $objects = sort($objects);
	        $objects = craOffsetKeysInvertArray($objects);
	    }

	    if($sorting == 5)
	        $objects = craOffsetKeysRandArray($objects);

	    $framesOffset *= $direction;

	    float $offset;

        float $craCount = 1;

        for( $obj in $objects )
        {
            float $craOffsetKeysSelectedKeys[] = `keyframe -q -selected $obj`;
            string $craOffsetKeysSelectedKeysFlag = "";

            if($mode == 2)
            {
                if($changeMode == " -tc ")
                    $offset = int(rand($framesOffset + (1*$direction)));
                else
                    $offset = rand($framesOffset + (1*$direction));
            }
            else if($mode == 1)
            {
                if(`iconTextCheckBox  -q -v craLinearOffsetCB`)
                  $offset += $framesOffset;

                if(`iconTextCheckBox  -q -v craSlopeOffsetCB`)
                {
                	if($craCount ==1)
                		$offset += $framesOffset;
                	else
                    	$offset += ($framesOffset/(size($objects)/$craCount));
                }

                if(`iconTextCheckBox  -q -v craSlopeDownOffsetCB`)
                {
                	if($craCount ==1)
                		$offset += $framesOffset;
                	else
                		$offset += ($framesOffset/$craCount);
                }
            }
            else
                $offset = $framesOffset;

		    if(size($craOffsetKeysSelectedKeys))
		    {
		        for($key in $craOffsetKeysSelectedKeys)
		            $craOffsetKeysSelectedKeysFlag+= (" -t " + string($key));
		        $craOffsetKeysSelectedKeysFlag+=" ";
		    }
            else
            {
                if ($timeRange[1] > ($timeRange[0]+1))
                {
                    string $framesRange = ("\"" + $timeRange[0] + ":" + $timeRange[1] + "\"");
                    $craOffsetKeysSelectedKeysFlag+= (" -t " + $framesRange);
                    $craOffsetKeysSelectedKeysFlag+=" ";
                }
            }

		    $cmd = "keyframe -e -abd 1 -option \"over\" -r" + $changeMode + $offset + " " + $craOffsetKeysAttrs + $craOffsetKeysSelectedKeysFlag + $obj;
		    eval($cmd);
            $craCount+=1;
        }
        // dgdirty -a;
	}
}

global proc string[] craOffsetKeysInvertArray( string $array[] )
{
    string $crainvertedArray[];

    for ($crai=size($array)-1,$j=0;$crai>=0;$crai--,$j++)
        $crainvertedArray[$j] = $array[$crai];

    return $crainvertedArray;
}

global proc string[] craOffsetKeysRandArray( string $stringArray[] )
{
    int $arraySize = size($stringArray);
    python("list = range(" + ($arraySize) + ")");
    python("import random");
    python("random.shuffle(list)");
    int $randomIndex[] = python("list");

    for( $crai=0;$crai<($arraySize);$crai++)
        $randomArray[$crai] =  strip($stringArray[$randomIndex[$crai]]);

    return $randomArray;
}

global proc string[] craOffsetKeysGetCBAttrs()
{
    if(size(`channelBox -q -sma mainChannelBox`)||size(`channelBox -q -ssa mainChannelBox`)||size(`channelBox -q -sha mainChannelBox`))
	{
	    if(size(`channelBox -q -sma mainChannelBox`))
	        return `channelBox -q -sma mainChannelBox`;
	    if(size(`channelBox -q -ssa mainChannelBox`))
	        return `channelBox -q -ssa mainChannelBox`;
	    if(size(`channelBox -q -sha mainChannelBox`))
	        return `channelBox -q -sha mainChannelBox`;
	}
	else
	    return {};
}

global proc craOffsetKeysCycle(int $mode)
{
    string $craOffsetKeysCBAttrs[] =`craOffsetKeysGetCBAttrs`;
    string $craOffsetKeysSelectedObjects[] = `ls -sl -l`;
    string $craOffsetKeysSelectedAnimCurves[] = `ls -sl -type "animCurveTU" -type "animCurveTA" -type "animCurveTT" -type "animCurveTL"`;
    string $craOffsetKeysSelectedCurves[];

    if(size($craOffsetKeysCBAttrs) || size($craOffsetKeysSelectedAnimCurves))
    {
        if(size($craOffsetKeysCBAttrs))
        {
            for($craOffsetKeysCBAttr in $craOffsetKeysCBAttrs)
            {
                for($craOffsetKeysSelectedObject in $craOffsetKeysSelectedObjects)
                {
                    string $craCurrentObjectCnnections[] = `listConnections -d 0 -p 0 -s 1 -c 0 ($craOffsetKeysSelectedObject + "." + $craOffsetKeysCBAttr)`;
                    $craOffsetKeysSelectedCurves[size($craOffsetKeysSelectedCurves)] = $craCurrentObjectCnnections[0];
                }
            }
        }
        else
            $craOffsetKeysSelectedCurves = $craOffsetKeysSelectedAnimCurves;

        for($craOffsetKeysSelectedCurve in $craOffsetKeysSelectedCurves)
        {
            switch ($mode)
            {
                case 0:
                // 0 constant
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".preInfinity") 0`);
                break;
                case 1:
                // 4 cycle with offset
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".preInfinity") 4`);
                break;
                case 2:
                // 3 cycle
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".preInfinity") 3`);
                break;
                case 3:
                // 3 cycle
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".postInfinity") 3`);
                break;
                case 4:
                // 4 cycle with offset
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".postInfinity") 4`);
                break;
                case 5:
                // 0 constant
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".postInfinity") 0`);
                break;
                case 6:
                // 1 linear
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".preInfinity") 1`);
                break;
                case 7:
                // 5 oscillate
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".preInfinity") 5`);
                break;
                case 8:
                // 1 linear
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".postInfinity") 1`);
                break;
                case 9:
                // 5 oscillate
                catchQuiet(`setAttr ($craOffsetKeysSelectedCurve + ".postInfinity") 5`);
                break;
            }
        }
    }
    else
    {
        if(size($craOffsetKeysSelectedObjects))
        {
            switch ($mode)
            {
                case 0:
                setInfinity -pri "constant";
                break;
                case 1:
                setInfinity -pri "cycleRelative";
                break;
                case 2:
                setInfinity -pri "cycle";
                break;
                case 3:
                setInfinity -poi "cycle";
                break;
                case 4:
                setInfinity -poi "cycleRelative";
                break;
                case 5:
                setInfinity -poi "constant";
                break;
                case 6:
                setInfinity -pri "linear";
                break;
                case 7:
                setInfinity -pri "oscillate";
                break;
                case 8:
                setInfinity -poi "linear";
                break;
                case 9:
                setInfinity -poi "oscillate";
                break;
            }
        }
    }
}

global proc string[] craGetAnimated(int $mode, string $type)
{
    string $aniObjects[];
    string $objects[];

    if(size(`ls -sl`))
        $objects = `ls -sl`;
    else
    {
        if($mode)
        {
            if($type == "nurbsCurve")
            {
                string $shapes[] = `ls -type $type`;
                $objects = `listRelatives -p $shapes`;
            }
            else
                $objects = `ls -type $type`;
        }
        else
            $objects = `ls`;
    }

    for ($i in $objects)
    {
        if(size(`keyframe -q $i`))
            if(!size(`ls -type "animCurve" $i`))
                $aniObjects[size($aniObjects)] =  $i;
    }

    if(!size($aniObjects))
        warning "no animated nodes found";
    return $aniObjects;
}

global proc craGetCurrentChannelValue()
{
    string $sel[] = `ls -sl`;
    string $craOffsetKeysCBAttrs[] =`craOffsetKeysGetCBAttrs`;
    float $value;
    if (size($craOffsetKeysCBAttrs))
    {
        $value = `getAttr ($sel[0] + "." + $craOffsetKeysCBAttrs[0])`;
        floatField -e -v `abs($value)` craOffsetKeysFramesIF;
    }
    else
        floatField -e -v 1 craOffsetKeysFramesIF;
}

global proc craSetZeroCurrentFrame( int $mode)
{
	int $cbPrecision = `optionVar -query channelsPrecision`;
    string $sel[] = `ls -sl`;
    string $craOffsetKeysCBAttrs[] =`craOffsetKeysGetCBAttrs`;
    float $value;
    float $currentValue = `floatField -q -v craOffsetKeysFramesIF`;
    int $randomValue = `iconTextCheckBox -q -v craRandomOffsetCB`;
    iconTextCheckBox -e -v 0 craRandomOffsetCB;

    if(size($sel))
    {
        undoInfo -chunkName "craOffsetKeysUndo" -openChunk;
        for( $s in $sel)
        {
    	    if(size($craOffsetKeysCBAttrs))
    	    {
    	        for ($attr in $craOffsetKeysCBAttrs)
    	        {
    	            $value = 0;
    	            $value = `getAttr ($s + "." + $attr)`;
    	            select -r ($s + "." + $attr);
    	            floatField -e -pre $cbPrecision -v `abs($value)` craOffsetKeysFramesIF;

    	            if($value <= 0)
    	                craOffsetKeysWrapper 1 " -vc ";
    	            if($value > 0)
    	                craOffsetKeysWrapper -1 " -vc ";
    	        }
    	    }
    	    else
    	    {
    	    	 int $mods = `getModifiers`;
    		    // shift
    			if ($mods == 1 || $mode == 1)
    				$craOffsetKeysCBAttrs = {"tx", "ty", "tz"};
    			// control+shift
                else if ($mods == 5 || $mode == 5)
    				$craOffsetKeysCBAttrs = {"tx", "tz"};
                // control
                else if ($mods == 4  || $mode == 4)
                    $craOffsetKeysCBAttrs = {"rx", "ry", "rz"};
                // alt
                else if ($mods == 8  || $mode == 8)
                    $craOffsetKeysCBAttrs = {"sx", "sy", "sz"};
    			else
    	    		$craOffsetKeysCBAttrs = {"tx", "ty", "tz", "rx", "ry", "rz"};

    	    	for ($attr in $craOffsetKeysCBAttrs)
    	        {
    	            $value = `getAttr ($s + "." + $attr)`;
    	            if($mods == 8)
    	            	$value-=1;

    	            select -r ($s + "." + $attr);

    	            floatField -e -pre $cbPrecision -v `abs($value)` craOffsetKeysFramesIF;

    	            if($value < 0)
    	                craOffsetKeysWrapper 1 " -vc ";
    	            if($value > 0)
    	                craOffsetKeysWrapper -1 " -vc ";

                    if(size(`keyframe -q`) && $mods != 8)
                        setKeyframe -v 0;
                    else if ($mods == 8)
                        setKeyframe -v 1;
    	        }
    	    }
        }

	    floatField -e -pre 3 -v $currentValue craOffsetKeysFramesIF;
        iconTextCheckBox -e -v $randomValue craRandomOffsetCB;
        select -r $sel;

        undoInfo -closeChunk;
    }
    else
    	warning "nothing selected";
}

