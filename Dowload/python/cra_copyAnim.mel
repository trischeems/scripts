// cra_pasteAnim //////////////////////////////////////////////////// Inicio

global proc cra_pasteAnim()
{
	if (!size (`ls -sl`))
		confirmDialog -ma "center" -t "  No animation curves." -button "OK" -db "OK" -m "No object(s) selected to paste animation to.";
	else 
	{
		string $cra_input[] = `ls -sl`;
		string $cra_inputAttr[];
		float $cra_key[]; 
		float $cra_offsetCBChannel = `floatFieldGrp -q -v1 copyNodeChannelFFG`;
		string $objectAttr[];
		// string $curvasSalvadas = `text -q -l animCurvesGuardadas`;
		string $curvasSalvadas = `nodeIconButton -q -l animCurvesGuardadas`;
		int $numTokens = `tokenize $curvasSalvadas "." $objectAttr`;
		
		if (size(`channelBox -q -hol mainChannelBox`) != 0 && size(`channelBox -q -sha mainChannelBox`) != 0) 
		{
			// $cra_input = `channelBox -q -hol mainChannelBox`;
			$cra_inputAttr = `channelBox -q -sha mainChannelBox`;
		}
		else if (size(`channelBox -q -sol mainChannelBox`) != 0 && size(`channelBox -q -ssa mainChannelBox`) != 0) 
		{
			// $cra_input = `channelBox -q -sol mainChannelBox`;
			$cra_inputAttr = `channelBox -q -ssa mainChannelBox`;
		}
		else if (size(`channelBox -q -mol mainChannelBox`) != 0 && size(`channelBox -q -mol mainChannelBox`) != 0)
		{
			// $cra_input = `channelBox -q -mol mainChannelBox`;
			$cra_inputAttr = `channelBox -q -sma mainChannelBox`;
		}

		// print "\n$cra_input1\n";
		// print $cra_input;

		// print "\n$cra_input2\n";
		// print $cra_input;
		if(`optionMenu -q -sl selectionOrderOM` == 1)
			stringArrayInsertAtIndex(0, $cra_input, $objectAttr[0]);		
		else 
			$cra_input[size($cra_input)] = $objectAttr[0];
	
		if(`optionMenu -q -sl selectionOrderOM` == 2)
			$cra_input = `craRandArray($cra_input)`;
		
		if(`optionMenu -q -sl selectionOrderOM` == 3)
			$cra_input = `craSortArray($cra_input)`;

		if(`optionMenu -q -sl selectionOrderOM` == 4)
		{
			string $leadObject = $cra_input[size($cra_input)-1];
			$cra_input= craSortArray($cra_input);
			stringArrayRemoveAtIndex(0, $cra_input);
			stringArrayInsertAtIndex(size($cra_input), $cra_input, $leadObject);
			$cra_input = cra_invertArray($cra_input);
		}
		
		if(!size($cra_inputAttr))
			$cra_inputAttr[0] = $objectAttr[1];

		// print "\n$cra_input3\n";
		// print $cra_input;


		for ($cra_attr in $cra_inputAttr)
		{
			for ($cra_i = 1, $cra_offset = 0; $cra_i<size($cra_input); $cra_i++, $cra_offset += $cra_offsetCBChannel) 
			{
				if (`checkBox -q -v cbRandInput`) 
				{
					seed $cra_i;
					float $cra_val1 = (`intFieldGrp -q -value1 rndOffsetNodeFF`);
					float $cra_val2 = (`intFieldGrp -q -value2 rndOffsetNodeFF`);
					$cra_offsetCBChannel = trunc(rand($cra_val1, $cra_val2));
				}
				
				float $currentAttrValue = `getAttr ($cra_input[$cra_i] + "." + $cra_attr)`;
				$cra_lastKey = $cra_i;
				$cra_key = `keyframe -q $objectAttr[0]`;
				copyKey -at $objectAttr[1] $objectAttr[0];
				// int $cra_fallOff = ($cra_key[0] + $cra_offsetCBChannel + $cra_offset);
				int $cra_fallOff = ($cra_key[0] + $cra_offset);
				
				pasteKey -option "replaceCompletely" -at $cra_attr -time $cra_fallOff $cra_input[$cra_i];
				
				if(`checkBox -q -v craMOCB`)
				{
					keyframe -q -at ($cra_input[$cra_i] + "." + $cra_attr);
					float $offsetValue = `getAttr ($cra_input[$cra_i] + "." + $cra_attr)`;
					float $diffValue = $currentAttrValue - $offsetValue;
					keyframe -e -relative -valueChange $diffValue ($cra_input[$cra_i] + "." + $cra_attr);
				}
			}
		}
	}
}

