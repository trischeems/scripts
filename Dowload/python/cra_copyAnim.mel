// cra_copyAnimDo //////////////////////////////////////////////////// Inicio
global proc cra_copyAnimDo()
{
	string $cra_input[] = ls("-sl");
	string $cra_inputAttr[];
	float $cra_key[]; 
	
	float $cra_offsetCBChannel = `floatFieldGrp -q -v1 copyNodeChannelFFG`;

	if(!size($cra_input))
	{
		craCopyAnimInViewMessage "Select some objects" "";
		error "Select some objects";
	}
	
	if (size(`channelBox -q -hol mainChannelBox`) != 0 && size(`channelBox -q -sha mainChannelBox`) != 0) 
		$cra_inputAttr = `channelBox -q -sha mainChannelBox`;
	if (size(`channelBox -q -sol mainChannelBox`) != 0 && size(`channelBox -q -ssa mainChannelBox`) != 0) 
		$cra_inputAttr = `channelBox -q -ssa mainChannelBox`;
	if (size(`channelBox -q -mol mainChannelBox`) != 0 && size(`channelBox -q -mol mainChannelBox`) != 0)
		$cra_inputAttr = `channelBox -q -sma mainChannelBox`;
	
	if(!size($cra_inputAttr))
	{
		$cra_tempInputAttr = `listAttr -k $cra_input[size($cra_input)-1]`;
		
		for($m=0,$j=0;$m< size($cra_tempInputAttr);$m++)
		{
			string $tempConnections[]= `listConnections -t "animCurve" ($cra_input[size($cra_input)-1] +"." + $cra_tempInputAttr[$m])`;
			if($tempConnections[0] != "")
			{
				$cra_inputAttr[$j] = $cra_tempInputAttr[$m];
				$j++;
			}
		}
	}
	
	if(size($cra_input) < 2 || !size($cra_inputAttr)) 
	{
		craCopyAnimInViewMessage "Object and or attributes missing" "\nSelect your objects in order. Then select the source animation object.\nYou can select some attribute(s) in the ChannelBox to copy only those";
		// confirmDialog -ma "center" -t "  Object and or attributes missing." -button "OK" -db "OK" -m "Select your objects in order. Then select the source animation object.\t\nYou can select some attribute(s) in the ChannelBox to copy only those.";
	}
	else
	{
		// print "\n$cra_input\n";
		// print $cra_input;

		if(`optionMenu -q -sl selectionOrderOM` == 1)
		{
			string $leadObject = $cra_input[size($cra_input)-1];
			stringArrayRemoveAtIndex(size($cra_input)-1, $cra_input);
			stringArrayInsertAtIndex(0, $cra_input, $leadObject);
		}
		
		if(`optionMenu -q -sl selectionOrderOM` == 2)
		{
			$cra_input = `craRandArray($cra_input)`;
		} 		
		
		if(`optionMenu -q -sl selectionOrderOM` == 3)
			$cra_input = `craSortArray($cra_input)`;
		
		if(`optionMenu -q -sl selectionOrderOM` == 4)
		{
			string $leadObject = $cra_input[size($cra_input)-1];
			$cra_input= craSortArray($cra_input);
			stringArrayRemoveAtIndex(0, $cra_input);
			stringArrayInsertAtIndex(size($cra_input), $cra_input, $leadObject);
			$cra_input = cra_invertArray($cra_input);
		}

		// print "\n$cra_input1\n";
		// print $cra_input;
		
		for ($cra_attr in $cra_inputAttr)
		{
			for ($cra_i = 1; $cra_i<size($cra_input); $cra_i++) 
			{
				seed $cra_i;
				if (`checkBox -q -v cbRandInput`) 
				{
					float $cra_val1 = (`floatFieldGrp -q -value1 rndOffsetNodeFF`);
					float $cra_val2 = (`floatFieldGrp -q -value2 rndOffsetNodeFF`);
					$cra_offsetCBChannel = trunc(rand($cra_val1, $cra_val2));
				}

				float $currentAttrValue = `getAttr ($cra_input[$cra_i] + "." + $cra_attr)`;
				
				$cra_lastKey = $cra_i - 1;
				$cra_key = `keyframe -q $cra_input[$cra_lastKey]`;
				
				copyKey -at $cra_attr $cra_input[$cra_lastKey];
				float $cra_fallOff = ($cra_key[0] + $cra_offsetCBChannel);
				pasteKey -option "replaceCompletely" -at $cra_attr -time $cra_fallOff $cra_input[$cra_i];
				
				if(`checkBox -q -v craMOCB`)
				{
					keyframe -q -at ($cra_input[$cra_i] + "." + $cra_attr);
					float $offsetValue = `getAttr ($cra_input[$cra_i] + "." + $cra_attr)`;
					float $diffValue = $currentAttrValue - $offsetValue;
					keyframe -e -relative -valueChange $diffValue ($cra_input[$cra_i] + "." + $cra_attr);
				}
			}
		}
	}
}

// cra_copyAnimDo //////////////////////////////////////////////////// Final



