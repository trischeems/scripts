		floatFieldGrp 			
			-numberOfFields 1
			-label "Offset " -extraLabel " frames"
			-ann "Number of frames to vary for each object" 
			-cw3 38 35 20
			-adj 3
			-pre 2
			-value1 1
		copyNodeChannelFFG;
		
		popupMenu;
			menuItem -label "  0 frame" -command "floatFieldGrp -e -value1 0 copyNodeChannelFFG;";
			menuItem -label "  1 frame" -command "floatFieldGrp -e -value1 1 copyNodeChannelFFG;";
			menuItem -label "  2 frames" -command "floatFieldGrp -e -value1 2 copyNodeChannelFFG;";
			menuItem -label "  3 frames" -command "floatFieldGrp -e -value1 3 copyNodeChannelFFG;";
			menuItem -label "  4 frames" -command "floatFieldGrp -e -value1 4 copyNodeChannelFFG;";
			menuItem -label "  5 frames" -command "floatFieldGrp -e -value1 5 copyNodeChannelFFG;";
			menuItem -label "  6 frames" -command "floatFieldGrp -e -value1 6 copyNodeChannelFFG;";
			menuItem -label "  7 frames" -command "floatFieldGrp -e -value1 7 copyNodeChannelFFG;";
			menuItem -label "  8 frames" -command "floatFieldGrp -e -value1 8 copyNodeChannelFFG;";
			menuItem -label "  9 frames" -command "floatFieldGrp -e -value1 9 copyNodeChannelFFG;";
			menuItem -label "10 frames" -command "floatFieldGrp -e -value1 10 copyNodeChannelFFG;";
			menuItem -label "15 frames" -command "floatFieldGrp -e -value1 15 copyNodeChannelFFG;";
			menuItem -label "20 frames" -command "floatFieldGrp -e -value1 20 copyNodeChannelFFG;";	
		setParent -m ..;

		nodeIconButton -h 20 -w 20 -i (`getApplicationVersionAsFloat` > 2015 ? "help.png" : "pickOtherObj.png") -ann "About" -c "craCopyAnimAboutScript";	
	setParent ..;
	
	rowLayout -nc 2;
		checkBox 
		-l "Random"
		-bgc 0.2 0.2 0.2
		-ann "Offset previous object animation within this range"
		-h 22
		-onc "floatFieldGrp -e -en 1 rndOffsetNodeFF;floatFieldGrp -e -en 0 copyNodeChannelFFG;"
		-ofc "floatFieldGrp -e -en 0 rndOffsetNodeFF; floatFieldGrp -e -en 1 copyNodeChannelFFG;"
		cbRandInput;
		
		floatFieldGrp 
		-bgc 0.2 0.2 0.2
		-en 0
		-h 22
		-cw2 35 35
		-numberOfFields 2
		-pre 2
		-value1 0
		-value2 5
		rndOffsetNodeFF;
	setParent ..;
	
	separator -h 2 -st "none";
	
	optionMenu selectionOrderOM;
		menuItem -l "order as selected" -c "";
		menuItem -l "order randomized" -c "";
		menuItem -l "sorted by name [A-Z]" -c "";
		menuItem -l "sorted by name [Z-A]" -c "";
	setParent -m;

	separator -h 2 -st "none";
	
	checkBox -v 1 -l " Mantain Xform Offset" -ann "Relative transform mode. Also affects time." craMOCB;
	
	separator  -h 2 -st "none";
	
	rowLayout -nc 3; 
	nodeIconButton 
		-w 46
		-st "iconOnly"
		-i "skinWeightCopy.png"
		-l "Copy animation" 
		-bgc .7 .7 .7 
		-ann "Select the objects you want to copy animation onto (selection order is important).\nThen select the object to copy the animation from\nYou can also select which attributes to copy in the Channel Box (optional)"
		-c "cra_copyAnimDo()" 
	copiarButton;

	nodeIconButton  
		-st "iconOnly"
		-i "save.png"
		-w 46
		-l "Store animation" 
		-ann "Stores the object.attribute animation curve selected in the Channel Box."
		-bgc .7 .7 .7 
		-c "cra_storeAnim()" 
	guardarButton;
	
	nodeIconButton  
		-st "iconOnly"
		-i "skinWeightPaste.png"
		-l "Paste animation"
		-en 0
		-w 46
		// -h 20
		-bgc .7 .7 .7 
		-c "cra_pasteAnim()"
		-ann "Select object(s) and attribute(s) to paste animation on."
	pegarButton;


	setParent ..;
	
	separator -h 2 -st "none";
	// text -bgc 0.4 0.4 0.4 -en 0 -h 20 -l "object.attribute" -ann "Currently stored object.attribute" animCurvesGuardadas;
	// button -bgc 0.4 0.4 0.4 -en 0 -h 20 -l "object.attribute" -ann "Currently stored object.attribute" animCurvesGuardadas;
	nodeIconButton -style "iconAndTextHorizontal"  -i "greenSelect" -bgc 0.4 0.4 0.4 -en 0 -h 20 -l "object.attribute" -ann "Currently stored object.attribute" animCurvesGuardadas;
	
	window -e
		-wh 158 153
		-s 0
		-mxb 0
		-mnb 0
		-tlb 0
	cra_copyAnimUI;

	showWindow cra_copyAnimUI;
