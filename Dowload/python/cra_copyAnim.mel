global proc cra_storeAnim()
{
	string $cra_input[] = `ls -sl`;
	string $cra_inputAttr[];
	
	float $cra_offsetCBChannel = `floatFieldGrp -q -v1 copyNodeChannelFFG`;
	
	if (size(`channelBox -q -hol mainChannelBox`) != 0 && size(`channelBox -q -sha mainChannelBox`) != 0) 
		$cra_inputAttr = `channelBox -q -sha mainChannelBox`;
	if (size(`channelBox -q -sol mainChannelBox`) != 0 && size(`channelBox -q -ssa mainChannelBox`) != 0) 
		$cra_inputAttr = `channelBox -q -ssa mainChannelBox`;
	if (size(`channelBox -q -mol mainChannelBox`) != 0 && size(`channelBox -q -mol mainChannelBox`) != 0)
		$cra_inputAttr = `channelBox -q -sma mainChannelBox`;
	
	if(!size($cra_inputAttr) || size($cra_inputAttr) > 1) 
	{
		// confirmDialog -ma "center" -t "  Source object.attribute missing" -button "OK" -db "OK" -m "Select an object and an attribute in the Channel Box\t\nwhich animation curve you want to store.";
		craCopyAnimInViewMessage "Source object/attribute missing:" "\nSelect an attribute in the Channel Box\nwhich animation curve you want to store.";
	}
	else 
	{
		string $cra_animCurves[];
		
		for ($cra_i = 0; $cra_i < size($cra_inputAttr); $cra_i++) 
		{
			$cra_tempAnimCurves = `listConnections -s 1 ($cra_input[0] + "." + $cra_inputAttr[$cra_i])`;
			if (size ($cra_tempAnimCurves) != 0) $cra_animCurves[$cra_i] = $cra_tempAnimCurves[0];
		}
		
		if (!size ($cra_animCurves))
		{
			craCopyAnimInViewMessage "Wrong attribute: " "The selected attribute is not driven by an animation curve";
			// confirmDialog -ma "center" -t "  No animation curves." -button "OK" -db "OK" -m "The selected attribute is not controlled by an animation curve.";
		}
		else 
		{
			string $objectAttr[];
			int $numTokens = `tokenize $cra_input[0] "|" $objectAttr`;
			
			// text -e -en 1 -ann ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) -l ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) animCurvesGuardadas;
			// button -e -en 1 -c ("select -r " + $objectAttr[$numTokens-1]) -ann ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) -l ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) animCurvesGuardadas;
			nodeIconButton -e -en 1 -c ("select -r " + $objectAttr[$numTokens-1]) -ann ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) -l ($objectAttr[$numTokens-1] + "." + $cra_inputAttr[0]) animCurvesGuardadas;
			nodeIconButton -e -en 1 pegarButton;
		}
	}
}

