global proc string[] cra_invertArray ( string $array[] ) 
{
	string $leadObject = $array[size($array)-1];
	stringArrayRemoveAtIndex((size($array)-1), $array);

	string $invertedArray[];

	for ($i=(size($array)-1);$i>=0;$i--)
		$invertedArray[$i] = $array[(size($array)-1)-$i];

	stringArrayInsertAtIndex(0, $invertedArray, $leadObject);	
	
	return $invertedArray;

}


global proc string[] craRandArray(string $stringArray[])
{
	string $leadObject = $stringArray[size($stringArray)-1];
	stringArrayRemoveAtIndex((size($stringArray)-1), $stringArray);

	int $arraySize = size($stringArray);
	python("l = range(" + ($arraySize) + ")");
	python("import random");
	python("random.shuffle (l)");
	int $randomIndex[] = python("l");
	
	for( $i=0;$i<($arraySize);$i++)
		$randomArray[$i] =$stringArray[$randomIndex[$i]];
	
	stringArrayInsertAtIndex(0, $randomArray, $leadObject);
	return $randomArray;
}


global proc string[] craSortArray(string $stringArray[])
{
	string $leadObject = $stringArray[size($stringArray)-1];
	stringArrayRemoveAtIndex((size($stringArray)-1), $stringArray);
	$sortedArray = sort($stringArray);
	stringArrayInsertAtIndex(0, $sortedArray, $leadObject);
	
	return $sortedArray;
}

global proc craCopyAnimInViewMessage( string $message1, string $message2)
{
    if(`getApplicationVersionAsFloat` > 2013)
    {
        int $inViewStatus = `optionVar -q inViewMessageEnable`;
        optionVar -iv inViewMessageEnable true;
        print ($message1 + " " + $message2 + "\n");
		inViewMessage 
            -a .9 
            -smg ("<font color=\"orange\">" + $message1 + " </font><font color=\"limegreen\">" + $message2 + "</font>") 
            -pos "topCenter" 
            -bkc 0x00001100 
            -fade 
            -fadeInTime 100 -fadeStayTime 1500 
            -fadeOutTime 500;
        optionVar -iv inViewMessageEnable $inViewStatus;
    }
    else
    {
        print ($message1 + " " + $message2 + "\n");
        headsUpMessage -t 0.1 ($message1 + " " + $message2);
    }
}

